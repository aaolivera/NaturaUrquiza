    ViewBag.Title = Textos.Producto_Modificar;
}


<script type="text/javascript" src="~/Scripts/jQuery.FileUpload/jquery.iframe-transport.js"></script>
<script type="text/javascript" src="~/Scripts/jQuery.FileUpload/jquery.fileupload.js"></script>
@Scripts.Render("~/bundles/jqueryval")
<script type="text/javascript" src="@Url.Content("~/scripts/administracion.modificar.js")"></script>
@Html.Hidden("urlUpdate",Url.Action("Upload","Natura"))
@using (Ajax.BeginForm("Modificar", "Administracion", new AjaxOptions(), new { @class = "form-horizontal search-form", data_dialogo_titulo = Textos.Producto_Modificar, enctype = "multipart/form-data" }))
